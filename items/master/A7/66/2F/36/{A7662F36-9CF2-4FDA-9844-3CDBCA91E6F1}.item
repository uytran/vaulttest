----field----
field: {A61CC636-3272-4FB6-BCF9-762940047A78}
name: Query
key: query
content-length: 375

SELECT 
       TC.[TestId]
      ,TC.[ClusterId]
      ,TC.[FeatureName]
      ,TC.[FeatureValue]
  FROM [Testing_Clusters] TC 
	INNER JOIN [Testing_ClusterMembers] TCM ON TCM.ClusterId = TC.ClusterId	
  WHERE 
	TC.[TestId] = @TestId
  GROUP BY 
       TC.[TestId]
      ,TC.[ClusterId]
      ,TC.[FeatureName]
      ,TC.[FeatureValue]
ORDER BY TC.[ClusterId] ;
