----field----
field: {A61CC636-3272-4FB6-BCF9-762940047A78}
name: Query
key: query
content-length: 368

SELECT
	 tc.[GoalId]
	,tc. [TestSetId]
	,tc. [TestValues]
	,SUM(tc.COUNT) AS Conversion
	,SUM(mvt.[Visits]) as Visits
FROM
	 dbo.[Fact_TestConversions] tc
INNER JOIN
	dbo.[Fact_MvTesting] mvt
ON
	tc.TestSetId = mvt.TestSetId and
	tc.TestValues = mvt.TestValues
WHERE
	 tc.[TestSetId] = @TestId
GROUP BY
	 [GoalId],
	 tc.[TestSetId],
	 tc.[TestValues]
